<h1>Home</h1>

<div>
    {{#each productos}}
        <div class="card">
            <p> {{this.title}} </p>
            <p> {{this.description}} </p>
            <p> {{this.price}} </p>
            <p> {{this.img}} </p>
            <p> {{this.category}} </p>
            <p> {{this.code}} </p>
            <p> {{this.stock}} </p>
        </div>
    {{/each}}
</div>